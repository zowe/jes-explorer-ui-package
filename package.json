{
  "name": "explorer-ui-server",
  "version": "0.2.17",
  "description": "HTTP Server to host static files",
  "author": "IBM",
  "license": "EPL-2.0",
  "main": "src/index.js",
  "bin": {
    "explorer-ui-server": "src/index.js"
  },
  "publishConfig": {
    "registry": "https://zowe.jfrog.io/zowe/api/npm/npm-local-release/"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "cp -r configs dist/configs && cp -r src dist/src && cp -r public dist/public && cp *.json dist && cp README.md dist && cp LICENSE dist",
    "prebuild": "npm run clean",
    "clean": "rimraf dist && mkdirp dist",
    "lint": "eslint .",
    "test": "nyc mocha"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "flat": "^5.0.1",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^3.0.8",
    "lodash": "^4.17.21",
    "opener": "^1.5.2",
    "pathval": "^1.1.1",
    "stdio": "0.2.7"
  },
  "optionalDependencies": {
    "keyring_js": "~1.0.0"
  },
  "devDependencies": {
    "abort-controller": "^3.0.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "eslint": "^5.16.0",
    "mkdirp": "^1.0.3",
    "mocha": "^7.2.0",
    "mocha-jenkins-reporter": "^0.4.3",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "node-fetch": "^2.6.1",
